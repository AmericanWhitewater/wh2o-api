{
  "name": "wh2o-api",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "American Whitewater",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "tsc && node dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "prepare": "husky install",
    "lint": "eslint --ext .ts ./src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "db:push": "prisma db push",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "preinstall": "node checkEnvironment.js",
    "test:coverage:serve": "python3 -m http.server --directory coverage/lcov-report"
  },
  "dependencies": {
    "@fastify/aws-lambda": "3.3.0",
    "@fastify/swagger": "8.8.0",
    "@fastify/swagger-ui": "1.9.3",
    "@prisma/client": "5.1.0",
    "axios": "1.4.0",
    "dotenv": "16.3.1",
    "fastify": "4.15.0",
    "fastify-cors": "6.1.0",
    "fastify-plugin": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@faker-js/faker": "8.0.1",
    "@types/aws-lambda": "8.10.119",
    "@types/jest": "29.5.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "eslint": "8.45.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-mock-extended": "^3.0.5",
    "js-yaml": "4.1.0",
    "nodemon": "2.0.22",
    "pino-pretty": "^10.2.0",
    "prettier": "3.0.0",
    "prisma": "5.1.0",
    "prisma-json-schema-generator": "^4.0.0",
    "semver": "^7.5.4",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  }
}
